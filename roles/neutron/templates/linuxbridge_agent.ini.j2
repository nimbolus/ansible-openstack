# {{ansible_managed}}

[linux_bridge]
physical_interface_mappings = provider:{{neutron_provider_interface}}

[securitygroup]
firewall_driver = neutron.agent.linux.iptables_firewall.IptablesFirewallDriver
enable_security_group = true

[vxlan]
enable_vxlan = true
local_ip = {{neutron_overlay_ip_address}}
l2_population = true
